{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rulesDirectory": [],
  "rules": {
    "member-ordering": [
      true,
      {
        "order": [
          "static-field",
          "instance-field",
          "constructor"
          // "public-instance-method",
          // "protected-instance-method",
          // "private-instance-method"
        ]
      }
    ],
    "no-empty-interface": false
  },
  "jsRules": {}
}